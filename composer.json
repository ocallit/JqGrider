{
  "name": "ocallit/jqgrider",
  "description": "Util functions for jqGrid",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "ocallit/sqler": "*",
    "ext-mysqli": "*"
  },
  "preferred-install": {
    "ocallit/sqler": "source"
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Ocallit\\JqGrider\\": "src/"
    }
  }
}